{
  "name": "vue2-apollo-scaphold",
  "private": true,
  "description": "Vue2 and Apollo w Scaphold.io",
  "repository": "kristianmandrup/vue2-apollo-scaphold",
  "version": "0.1.0",
  "scripts": {
    "start": "babel-node ./src/server.js",
    "start-prod": "node ./lib/server.js",
