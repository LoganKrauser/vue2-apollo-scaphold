    "graphiql": "^0.7.8",
    "graphql": "^0.7.0",
    "graphql-tag": "^0.1.11",
    "isomorphic-fetch": "^2.2.1",
    "sync-request": "^2.0.1",
    "vue": "^2.1.5",
    "vue-apollo": "^1.1.0-beta.2",
    "vue-awesome": "^2.0.3",
    "vue-loader": "^9.8.1",
    "vue-material": "^0.2.0",
    "vue-router": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
